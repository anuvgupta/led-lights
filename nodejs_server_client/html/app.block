                            padding 8px 0
                            width 150px
                            text-align center
                            border-radius 4px
                            cursor default
                            color white
                            transition background-color 0.15s ease, opacity 0.1s ease, box-shadow 0.15s ease, -moz-box-shadow 0.15s ease, -webkit-box-shadow 0.15s ease
                            border 1px solid #ffffff
